{
  "desktop-plugin-version": "1.0.0-beta.1",
  "name": "Tailscale",
  "provider": "Tailscale Inc.",
  "icon": "tailscale.svg",
  "vm": {
    "composefile": "docker-compose.yaml"
  },
  "ui": {
    "dashboard-tab": {
      "title": "Tailscale",
      "root": "/ui",
      "src": "index.html"
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/darwin/hostname"
          },
          {
            "path": "/darwin/host-tailscale"
          }
        ],
        "windows": [
          {
            "path": "/windows/hostname.cmd"
          },
          {
            "path": "/windows/host-tailscale.cmd"
          }
        ],
        "linux": [
          {
            "path": "/linux/hostname.sh"
          },
          {
            "path": "/linux/host-tailscale.sh"
          }
        ]
      }
    ]
  }
}
